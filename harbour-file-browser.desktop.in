[Desktop Entry]
Type=Application
X-Nemo-Application-Type=silica-qt5
Name=File Browser
Icon=harbour-file-browser
Exec=harbour-file-browser %f
Categories=System;FileTools;FileManager
$$CONF_DESKTOP_MIME_TYPE

# Translators: the name has to be short enough not to be abbreviated
# by the system (launcher view).
Name[en]=File Browser
Name[fi]=Tiedostoselain
Name[de]=Dateiverwaltung
Name[pl]=Pliki

[X-HarbourBackup]
BackupPathList=.local/share/harbour-file-browser/:.config/harbour-file-browser

[X-Sailjail]
$$CONF_SAILJAIL_DETAILS
OrganizationName=harbour-file-browser
ApplicationName=harbour-file-browser
